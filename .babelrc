{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 1%", "last 10 versions", "not ie < 8"]
      },
      "useBuiltIns":true
    }], 
    "stage-0",
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
